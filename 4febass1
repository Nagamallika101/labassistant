#include <stdio.h>
#include<string.h>
#define MAXSIZE100
int main()
{
  char str[MAX SIZE];
  int i,len,vowel,consonant;
  printf("enter any string:");
  gets(str);
  vowel=0;
  consonant=0;
  len=Strlen(str);
  for(i=0;i<len;i++)
  {
      if((str[i]>='a'&&str[i]<='z')||(str[i]>='A'&&str[i]<='Z'))
      {
         switch(str[i])
         {
             case'a':
             case'A':
             case'e':
             case'E':
             case'i':
             case'I':
             case'o':
             case'O':
             case'u':
             case'U':
             vowel++;
             break;
             default:cosonant;
         }
      }
  }
  printf("total number of vowel=%d\n",vowel);
  printf("total number of consonant=%d\n",consonant);
  return 0;
}
         
       
