#include <stdio.h>

int main()
{
    int a[5];
    printf("%d enter the value for a");
    scanf("%d",&a);
    int *p=a;
    int i;
    int x;
    int sum=0;
    for(i=0;i<5;i++)
    { 
        x=*(p+i);
        sum=sum+x;
    }
    printf("%d",sum);

    return 0;
}
