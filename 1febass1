#include<stdio.h>
#define MAXROW  10
#define MAXCOL  10
void read matrix(int m[][MAXCOL],introw,intcol)
{
 int i,j;
 for(i=0;i<row;i++)
 {
   for(j=0;j<col;j++)
   {
     printf("enter element[%d%d]:",i+1,j+1);
     scanf("%d",&m[i][j]);
   }
 }
}
void print matrix("int m[][MAXCOL],introw,intcol")
{
  int i,j;
  for(i=0;i<row:i++)
  {
      for(j=0;j<col;j++)
      {
          printf("%d\t",m[i][j]);
      }
      printf("\n");
  }
}
