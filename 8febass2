#include <stdio.h>
void 
print 10 to 1(int);
int main()
{
    int n=10;
    printf("\n numbers from 10 to 1 are:");
    print 10 to 1(n);
    return 0;
}
 void print 10 to 1(int n)
 {
     if(n)
     print 10 to 1(n-1);
     else
     return; 
     printf("\n%d\n",n);
 }
    
